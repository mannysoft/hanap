{
    "name": "mannysoft/hanap",
    "description": "Laravel package API Result filtering, sorting & searching.",
    "keywords": [
        "laravel",
        "api",
        "search",
        "filter",
        "sorting",
        "search"
    ],
    "authors": [
        {
            "name": "Manny Isles"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "sofa/eloquence": "5.6.*"
    },
    "autoload": {
        "psr-0": {
            "Mannysoft\\Hanap\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mannysoft\\Hanap\\ServiceProvider"
            ],
            "aliases": {
                "Hanap": "Mannysoft\\Hanap\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
